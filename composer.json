{
	"name": "dlouvard/laravel-releasenote",
	"description": "Release Note for Laravel 5",
	"keywords": [
		"laravel",
		"release",
		"releasenote",
		"note"
	],
	"homepage": "https://github.com/dlouvard/laravel-releasenote",
	"require": {
		"php": "^7.2",
		"illuminate/database": "6.*",
		"illuminate/events": "6.*",
		"doctrine/dbal": "^2.10",
		"bluemountainteam/bootform": "6.*"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.0",
		"mockery/mockery": "^1.0",
		"satooshi/php-coveralls": "1.*"
	},
	"repositories": [{
			"type": "vcs",
			"url": "https://github.com/bluemountainteam/bootform"
		},
		{
			"type": "vcs",
			"url": "https://github.com/bluemountainteam/translation"
		}
	],
	"autoload": {
		"psr-4": {
			"Dlouvard\\Releasenote\\": "src/"
		}
	}
}
